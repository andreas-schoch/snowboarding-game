{
  "name": "wicked-snowman",
  "version": "0.1.0",
  "description": "A physics based snowboarding game.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.dev.js",
    "build": "rimraf dist && webpack --config webpack/webpack.prod.js",
    "build-dev": "rimraf dist && webpack --config webpack/webpack.dev.js",
    "bundle": "npm run build",
    "serve": "serve dist",
    "pages-predeploy": "npm run build",
    "pages-deploy": "gh-pages -d ./dist"
  },
  "keywords": [
    "box2d",
    "physics",
    "snowboard",
    "endless runner"
  ],
  "author": "Andreas Schoch",
  "license": "MIT",
  "devDependencies": {
    "copy-webpack-plugin": "^9.0.1",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.2.0",
    "rimraf": "^3.0.2",
    "serve": "^12.0.0",
    "ts-loader": "^9.2.4",
    "typescript": "^4.1.3",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.7.3",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "dependencies": {
    "box2d-wasm": "^6.0.4",
    "phaser": "^3.55.2",
    "simplex-noise": "^3.0.1",
    "simplify-js": "^1.2.4"
  }
}
