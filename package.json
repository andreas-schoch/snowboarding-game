{
  "name": "snowboarding-game",
  "version": "1.1.1",
  "description": "A physics based snowboarding game.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.dev.js",
    "build": "rimraf dist && webpack --config webpack/webpack.prod.js",
    "bundle": "npm run build",
    "serve": "serve dist",
    "pages-predeploy": "npm run build",
    "pages-deploy": "gh-pages -d ./dist"
  },
  "keywords": [
    "box2d",
    "physics",
    "snowboard",
    "game",
    "fun",
    "2d",
    "phaser"
  ],
  "author": "Andreas Schoch",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv-webpack": "^8.0.1",
    "gh-pages": "^6.1.0",
    "html-webpack-plugin": "^5.6.0",
    "rimraf": "^5.0.5",
    "serve": "^14.2.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "box2d-wasm": "^7.0.0",
    "firebase": "^10.7.1",
    "phaser": "^3.70.0",
    "phaser3-rex-plugins": "^1.60.8",
    "wasm-feature-detect": "^1.6.1"
  }
